
<div class="modal fade" id="hebMonths" tabindex="-1" role="dialog" aria-labelledby="hebMonths-label" aria-hidden="true">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="hebMonths-label">Hebrew Months</h5>
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
			</div>
			<div class="modal-body">

				<table class="table table-striped table-sm">
					<thead>
						<tr>
							<th>Month</th>
							<th>Hebrew</th>
							<th>Length</th>
							<th>Gregorian Equivalent</th>
							<th>Biblical Name</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<td>Nissan</td>
							<td><span class="float-right hebrew medium">ניסן</span></td>
							<td>30 days</td>
							<td>March-April</td>
							<td>Abib</td>
						</tr>
						<tr>
							<td>Iyar</td>
							<td><span class="float-right hebrew medium">אייר</span></td>
							<td>29 days</td>
							<td>April-May</td>
							<td>Ziv</td>
						</tr>
						<tr>
							<td>Sivan</td>
							<td><span class="float-right hebrew medium">סיון</span></td>
							<td>30 days</td>
							<td>May-June</td>
							<td>3rd</td>
						</tr>
						<tr>
							<td>Tammuz</td>
							<td><span class="float-right hebrew medium">תמוז</span></td>
							<td>29 days</td>
							<td>June-July</td>
							<td>4th</td>
						</tr>
						<tr>
							<td>Av</td>
							<td><span class="float-right hebrew medium">אב</span></td>
							<td>30 days</td>
							<td>July-August</td>
							<td>5th</td>
						</tr>
						<tr>
							<td>Elul</td>
							<td><span class="float-right hebrew medium">אלול</span></td>
							<td>29 days</td>
							<td>August-September</td>
							<td>6th</td>
						</tr>
						<tr>
							<td>Tishri</td>
							<td><span class="float-right hebrew medium">תשרי</span></td>
							<td>30 days</td>
							<td>September-October</td>
							<td>Ethanim</td>
						</tr>
						<tr>
							<td>Heshvan</td>
							<td><span class="float-right hebrew medium">חשון</span></td>
							<td>29 or 30 days</td>
							<td>October-November</td>
							<td>Bul</td>
						</tr>
						<tr>
							<td>Kislev</td>
							<td><span class="float-right hebrew medium">כסלו</span></td>
							<td>30 or 29 days</td>
							<td>November-December</td>
							<td>9th</td>
						</tr>
						<tr>
							<td>Tevet</td>
							<td><span class="float-right hebrew medium">טבת</span></td>
							<td>29 days</td>
							<td>December-January</td>
							<td>10th</td>
						</tr>
						<tr>
							<td>Shevat</td>
							<td><span class="float-right hebrew medium">שבט</span></td>
							<td>30 days</td>
							<td>January-February</td>
							<td>11th</td>
						</tr>
						<tr>
							<td>Adar</td>
							<td><span class="float-right hebrew medium">אדר א</span></td>
							<td>29 or 30 days</td>
							<td>February-March</td>
							<td>12th</td>
						</tr>
						<tr>
							<td>Adar II</td>
							<td><span class="float-right hebrew medium">אדר ב</span></td>
							<td>29 days</td>
							<td>March-April</td>
							<td>13th</td>
						</tr>

					</tbody>

				</table>

			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>
