<div class="row">
	<div class="col-6">
		<div class="card border-warning">
			<div class="card-body">
				<p class="card-text">@Col1</p>
				
			</div>
		</div>
	</div>

	<div class="col-6">
		<div class="card border-info">
			<div class="card-body">
				<p class="card-text">@Col2</p>
			</div>
		</div>

	</div>
</div>


@code {
	[Parameter] public RenderFragment? Col1 { get; set; } = default;
	[Parameter] public RenderFragment? Col2 { get; set; } = default;
}
