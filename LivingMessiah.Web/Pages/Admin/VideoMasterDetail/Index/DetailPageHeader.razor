@using Microsoft.AspNetCore.Components;

@inherits Fluxor.Blazor.Web.Components.FluxorComponent

@if (State!.Value.DetailPageHeaderVM is not null)
{
	<legend class="text-primary bg-light mb-0 p-2">
		<b>@State!.Value.DetailPageHeaderVM!.Label:</b> @State!.Value.DetailPageHeaderVM!.Value
	</legend>
}


@code {
	[Inject] private IState<IndexState>? State { get; set; }
}
