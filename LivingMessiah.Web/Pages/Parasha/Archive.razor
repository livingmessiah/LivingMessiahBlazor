@page "/parasha/Archive"

@using Page = LivingMessiah.Web.Links.Parasha
<PageTitle>@Page.Archive.Title</PageTitle>

<div class="pb-2 mt-4 mb-2 border-bottom">

	<h3><i class="@Page.Archive.Icon"></i> @Page.Archive.Title</h3>

	<BackButton Href="@Page.Index" Text="@Page.BackToButtonText"/>

	<div class="mt-3">
		<SfTab CssClass="">
			<TabItems>

				<TabItem>
					<ChildContent>
						<TabHeader Text="Leviticus"></TabHeader>
					</ChildContent>
					<ContentTemplate>
						<ArchiveTableStaticLev></ArchiveTableStaticLev>
					</ContentTemplate>
				</TabItem>

				<TabItem>
					<ChildContent>
						<TabHeader Text="Numbers"></TabHeader>
					</ChildContent>
					<ContentTemplate>
						<ArchiveTableStaticNum></ArchiveTableStaticNum>
					</ContentTemplate>
				</TabItem>

				<TabItem>
					<ChildContent>
						<TabHeader Text="Deuteronomy"></TabHeader>
					</ChildContent>
					<ContentTemplate>
						<ArchiveTableStaticDeu></ArchiveTableStaticDeu>
					</ContentTemplate>
				</TabItem>

				<TabItem>
					<ChildContent>
						<TabHeader Text="Genesis"></TabHeader>
					</ChildContent>
					<ContentTemplate>
						<ArchiveTableStaticGen></ArchiveTableStaticGen>
					</ContentTemplate>
				</TabItem>

			</TabItems>
		</SfTab>
	</div>

</div>
